{
	"schemaVersion": 1,
	"id": "tacz-fabric",
	"version": "${version}",
	"name": "TACZ-Fabric",
	"description": "This is an example description! Tell everyone what your mod is about!",
	"authors": [
		"Me!"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/tacz-fabric/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.tacz.guns.TACZFabric"
		],
		"client": [
			"com.tacz.guns.TACZFabricClient"
		]
	},
	"mixins": [
		"tacz-fabric.mixins.json",
		{
			"config": "tacz-fabric.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.15.11",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}